{
  "name": "bsml-generator",
  "version": "1.0.0",
  "description": "Create UIs and export them as Beatsaber Markup Language",
  "main": "src/js/app.js",
  "dependencies": {
    "tailwindcss": "^1.4.6"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
